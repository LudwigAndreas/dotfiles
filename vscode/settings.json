{
  "accessibility.signals.diffLineDeleted": {
    "sound": "off"
  },
  "accessibility.signals.diffLineInserted": {
    "sound": "off"
  },
  "accessibility.signals.lineHasError": {
    "sound": "off"
  },
  "accessibility.signals.lineHasFoldedArea": {
    "sound": "off"
  },
  "accessibility.signals.lineHasInlineSuggestion": {
    "sound": "off"
  },
  "accessibility.signals.noInlayHints": {
    "sound": "off"
  },
  "accessibility.signals.notebookCellCompleted": {
    "sound": "off"
  },
  "accessibility.signals.notebookCellFailed": {
    "sound": "off"
  },
  "accessibility.signals.onDebugBreak": {
    "sound": "off"
  },
  "accessibility.signals.taskCompleted": {
    "sound": "off"
  },
  "accessibility.signals.taskFailed": {
    "sound": "off"
  },
  "accessibility.signals.terminalCommandFailed": {
    "sound": "off"
  },
  "accessibility.signals.terminalQuickFix": {
    "sound": "off"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 3000,
  "terminal.integrated.env.osx": {
    "CW_NEW_SESSION": "1",
    "FIG_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.editor.empty.hint": "hidden",
  "editor.cursorWidth": 3,
  "editor.fontFamily": "'JetBrainsMono Nerd Font'",
  "editor.fontSize": 15,
  "editor.lineNumbers": "relative",
  "editor.tabSize": 4,
  "editor.fontLigatures": false,
  "editor.cursorSurroundingLines": 8,
  "accessibility.signals.progress": {
    "sound": "off"
  },
  "workbench.settings.applyToAllProfiles": [],
  "editor.minimap.enabled": false,
  "vim.vimrc.enable": false,
  "vim.vimrc.path": "~/.vimrc",
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindings": [
    // ------- Tab/Window/Buffer/Split actions -------
    {
      "before": ["<leader>", "w", "l"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["<leader>", "w", "j"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["<leader>", "w", "k"],
      "commands": ["workbench.action.splitEditorUp"]
    },
    {
      "before": ["<leader>", "w", "h"],
      "commands": ["workbench.action.splitEditorLeft"]
    },
    {
      "before": ["<leader>", "w", "m"],
      "commands": ["workbench.action.moveEditorToNextGroup"]
    },
    {
      "before": ["<leader>", "w", "b"],
      "commands": ["workbench.action.moveEditorToPreviousGroup"]
    },
    {
      "before": ["<leader>", "o", "a"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "c", "c"],
      "commands": ["workbench.action.openSettingsJson"]
    },
    {
      "before": ["<leader>", "c", "k"],
      "commands": ["workbench.action.openGlobalKeybindingsFile"]
    }
  ],
  // ------- Formatters -------
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.fontSize": 15
}
